main.page-wrapper.page-wrapper_enter
  .enter-box
    .enter-box__main
      h1.enter-box__title !{title}
      | !{inputEmail}
      | !{inputLogin}
      | !{inputName}
      | !{inputLastName}
      | !{inputPhone}
      | !{inputPassword}
      | !{inputPasswordConfirm}
